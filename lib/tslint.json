{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "object-literal-sort-keys": false,
      "interface-name": [ true, "never-prefix" ],
      "trailing-comma": [ false ],
      "variable-name": {
        "options": [
          "ban-keywords",
          "check-format",
          "allow-leading-underscore",
          "allow-pascal-case"
        ]
      }
  },
  "rulesDirectory": []
}